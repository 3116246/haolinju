				<div  style="text-align:center;border-bottom: 1px solid #999999;"><span style="font-weight:bold;">当前权限</span></div>
					<div style="padding-left:40px; overflow: auto; height: 310px;">
						{%for n in curFunc%}
						<li>{{n["name"]}}</li>
						{%endfor%}
					</div>
  			</div>
			  <div class="c_act_t1" style="float:left;width: 200px; background-color: #999999; color: rgb(255, 255, 255);padding-bottom: 10px;">
					<div  style="text-align:center;border-bottom: 1px solid #CCCCCC;"><span style="font-weight:bold;">{{up}} 权限</span></div>
					<div style="padding-left:40px; overflow: auto; height: 310px;">
						{%for n in upFunc%}
						<li>{{n["name"]}}</li>
						{%endfor%}
					</div>
  			</div>
  			<div class="clearBoth"></div>
			</div>  			
