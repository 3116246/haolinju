<div style="width:auto;height:301px;">
<table class="table table-bordered" style="border-bottom:0px;margin-top:33px;">
	      <thead style="background-color:#f0f0f0"><tr><th>计划</th><th>状态</th></tr></thead>
			 {% if data|length>0 %}
			 		<tbody>
			    {% for i in data %}
			        <tr>{#<td>{{i.start_time}}-{{i.end_time}}</td>#}
			        	  <td><a class="task_card" id="{{ i.id }}" href="#"  {% if(i.is_after_start_time>=0 or i.is_after_end_time>=0)%} style="color:#5199CB;"{% endif %}>{{i.title}}</a></td>
			            <td>{{i.is_finish|raw}}</td>
			        </tr>
			    {% endfor %}
			 {% else %}
			   {#<tr><td style="height:26px;"></td><td style="height:26px;"></td><td></td></tr>#}
			 {% endif %}
			 		<tr><td style="height:26px;" colspan="2"  align="center"><a  onclick="new_task();" style="cursor:pointer;" id="new_task_a">点击添加计划</a></a></td></tr>
			 </tbody>
</table>
</div>
{%if (pagecount>1) %}
	<div class="pagination" style="text-align:right;margin-top:25px;">
	<ul>
		<li><a {%if(pageindex!=0)%} class='active' {%endif%} href='javascript:get_table_page(1)'>首页</a></li>
	{% set pagestart= pageindex<=2?1:pageindex-2 %}
		{% set pageend=pagestart+3>pagecount?pagecount:pagestart+3%}
		{%for i in pagestart..pageend%}
	  <li><a {%if(pageindex==i)%} class='active' {%endif%} href='javascript:get_table_page({{i}})'>{{(i)}}</a></li>
	{%endfor%}
	  <li><a {%if(pageindex!=pagecount)%} class='active' {%endif%} href='javascript:get_table_page({{pagecount}})'>末页</a></li>
</ul>
</div>
{%endif%}
